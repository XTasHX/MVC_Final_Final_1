@model IEnumerable<MVC_Final_Final.Models.Docs.DocsContext>

@{
    ViewData["Title"] = "Download";
}

<form asp-controller="Docs" asp-action="Download" method="post"
      enctype="multipart/form-data">

    <input type="file" name="file" />
    <button type="submit">Download File</button>

</form>


    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">Doc Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr class="table-dark">
                <th scope="row">Dark</th>
                <td> <a asp-area="" asp-controller="Docs" asp-action="Download" asp-route-docNames=@item.DocNames>Download</a> </td>
                <td>Column content</td>
            </tr>
            }

        </tbody>
    </table>

